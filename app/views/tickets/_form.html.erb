<%= simple_form_for  ([@team, @match, @ticket]) do |f| %>
  <% if @team.id == @match.chlg_team_id %>
    <%= f.file_field :chlg_team_img1%>
    <%= f.file_field :chlg_team_img2%>
    <%= f.file_field :chlg_team_img3%>
    <%= f.file_field :chlg_team_img4%>
    <%= f.file_field :chlg_team_img5%>
  <% else %>
    <%= f.file_field :acpt_team_img1%>
    <%= f.file_field :acpt_team_img2%>
    <%= f.file_field :acpt_team_img3%>
    <%= f.file_field :acpt_team_img4%>
    <%= f.file_field :acpt_team_img5%>
  <% end %>
  <%= f.submit %>
<% end %>

