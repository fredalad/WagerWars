<h1>Team Name: <%= @team.name%></h1>
<% @team.user_id.each  do |value| %>
<% end %>
<% @user.each  do |value| %>
  <h1><%= value.username%></h1>
<% end %>

<h1><%= link_to "Edit Roster", team_users_path(@team.id)%></h1>
<h2><%= link_to "Find Match", team_matches_path(@team.id) %></h2>
<h2><%= link_to "View Upcoming Matches", team_matches_path(@team.id, :match_status => "upcoming") %></h2>
<h3><%= link_to "Destroy", team_path(@team), method: :delete, data: {confirm: "Are you sure?"} %> </h3>
